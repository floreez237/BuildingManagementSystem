            <ImageView fitHeight="52.0" fitWidth="56.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/icons8_back_arrow_32px.png"/>
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
      <Label layoutX="440.0" layoutY="129.0" text="Status:">
         <font>
            <Font name="Cambria" size="18.0"/>
