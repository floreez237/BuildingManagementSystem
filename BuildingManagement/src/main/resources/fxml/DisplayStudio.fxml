<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<Pane xmlns:fx="http://javafx.com/fxml/1" prefHeight="722.0" prefWidth="744.0" xmlns="http://javafx.com/javafx/8.0.141"
      fx:controller="buildingProject.controllers.DisplayStudioController">
   <children>
      <Label layoutX="49.0" layoutY="117.0" text="Room ID:">
         <font>
            <Font size="20.0"/>
