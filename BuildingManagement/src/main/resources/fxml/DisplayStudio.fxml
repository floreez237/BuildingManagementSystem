         <font>
            <Font name="Cambria" size="18.0"/>
         </font>
         <padding>
            <Insets left="30.0"/>
         </padding>
      </Label>
      <JFXTextField fx:id="tfPaintColor" layoutX="152.0" layoutY="228.0" prefHeight="31.0" prefWidth="580.0"/>
