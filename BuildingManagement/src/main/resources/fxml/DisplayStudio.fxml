                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
               </content>
            </TitledPane>
            <TitledPane fx:id="tpFurniture" animated="false" text="Furnitures:">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
               </content>
            </TitledPane>
         </panes>
      </Accordion>
      <JFXTextField fx:id="tfRent" layoutX="152.0" layoutY="171.0" prefHeight="31.0" prefWidth="220.0"/>
