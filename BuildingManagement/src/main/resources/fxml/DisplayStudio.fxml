      <Label layoutX="16.0" layoutY="308.0" text="Bedroom Area:">
         <font>
            <Font name="Cambria" size="18.0"/>
         </font>
      </Label>
      <Label layoutX="400.0" layoutY="308.0" prefHeight="22.0" prefWidth="110.0" text="Parlour Area:">
         <font>
            <Font name="Cambria" size="18.0"/>
         </font>
      </Label>
      <JFXTextField fx:id="tfParlourArea" layoutX="513.0" layoutY="293.0" prefHeight="31.0" prefWidth="220.0"/>
      <JFXTextField layoutX="152.0" layoutY="361.0" onAction="#tfKitchenArea" prefHeight="31.0" prefWidth="220.0"/>
      <JFXTextField layoutX="504.0" layoutY="361.0" onAction="#tftAreaToilet" prefHeight="31.0" prefWidth="220.0"/>
      <Label layoutX="410.0" layoutY="381.0" text="Toilet Area:">
         <font>
            <Font name="Cambria" size="18.0"/>
         </font>
      </Label>
      <Label layoutX="33.0" layoutY="381.0" text="Kitchen Area:">
         <font>
            <Font name="Cambria" size="18.0"/>
         </font>
      </Label>
      <JFXButton layoutX="561.0" layoutY="74.0" onAction="#onDisplay" prefHeight="43.0" prefWidth="91.0" text="Bills">
         <graphic>
            <ImageView fitHeight="24.0" fitWidth="38.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/icons8_bill_26px.png"/>
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
   </children>
</Pane>
