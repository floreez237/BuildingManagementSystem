         <font>
            <Font name="Cambria" size="18.0"/>
         </font>
         <padding>
            <Insets left="30.0"/>
         </padding>
      </Label>
      <Label layoutX="431.0" layoutY="176.0" prefHeight="22.0" prefWidth="71.0" text="Deposit:">
         <font>
            <Font name="Cambria" size="18.0"/>
         </font>
      </Label>
      <JFXTextField fx:id="tfDeposit" layoutX="522.0" layoutY="161.0" prefHeight="31.0" prefWidth="206.0"/>
