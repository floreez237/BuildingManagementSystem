                  <InnerShadow blurType="GAUSSIAN" choke="0.35" color="#110ee1" height="0.0" radius="4.0075" width="18.03" />
               </effect>
               <fill>
                  <LinearGradient endX="1.0" endY="1.0">
                     <stops>
                        <Stop color="#00e5ff" />
                        <Stop color="#0073ff" offset="1.0" />
                     </stops>
                  </LinearGradient>
               </fill>
               <cursor>
                  <Cursor fx:constant="HAND" />
