            </Label>
            <Label fx:id="lblUnPaidElectricity" alignment="CENTER_RIGHT" layoutX="126.0" layoutY="147.0" text="2" textFill="WHITE">
               <font>
                  <Font name="Bookman Old Style Bold" size="12.0" />
               </font>
            </Label>
         </children>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Group>
   </children>
   <padding>
      <Insets bottom="5.0" left="25.0" />
   </padding>
</Pane>
